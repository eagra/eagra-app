{
  "name": "eagra-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "watch": "vite",
    "build": "yarn typecheck && vite build",
    "build:prod": "NODE_ENV=production && vite build",
    "preview": "vite preview",
    "typecheck": "tsc",
    "lint": "yarn uniform lint",
    "lint:fix": "yarn uniform lint --fix",
    "graphql:schema:download": "apollo schema:download --endpoint=https://hasura-testnet.nstankov.com/v1/graphql src/graphql/schema.json",
    "graphql:types:generate": "apollo codegen:generate --localSchemaFile=src/graphql/schema.json --target=typescript"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11",
    "@emurgo/cardano-serialization-lib-browser": "^9.1.4",
    "@emurgo/cip14-js": "^3.0.1",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.0.2",
    "buffer": "^6.0.3",
    "framer-motion": "^4",
    "graphql-request": "^3.7.0",
    "graphql-tag": "^2.12.6",
    "jotai": "^1.6.1",
    "p5": "^1.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "recharts": "^2.1.8",
    "swr": "^1.1.1"
  },
  "devDependencies": {
    "@dauta/uniform": "^0.5.2",
    "@types/p5": "^1.3.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/recharts": "^1.8.23",
    "@vitejs/plugin-react": "^1.2.0",
    "apollo": "^2.33.9",
    "typescript": "^4.5.2",
    "vite": "^2.8.6",
    "vite-plugin-top-level-await": "^0.0.4",
    "vite-plugin-wasm": "^0.0.2",
    "yargs-parser": "^21.0.0"
  }
}
