{
  "name": "eagra-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn typecheck && node .esbuild/build",
    "build:prod": "NODE_ENV=production node .esbuild/build --minify --analyze",
    "watch": "node .esbuild/watch",
    "typecheck": "tsc --noEmit",
    "graphql:schema:download": "apollo schema:download --endpoint=https://graphql-api.mainnet.dandelion.link src/graphql/schema.json",
    "graphql:types:generate": "apollo codegen:generate --localSchemaFile=src/graphql/schema.json --target=typescript"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.1.1",
    "@chakra-ui/react": "^1.7.3",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11",
    "@emurgo/cardano-serialization-lib-browser": "^8.1.0",
    "@emurgo/cip14-js": "^3.0.1",
    "buffer": "^6.0.3",
    "framer-motion": "^4",
    "graphql-request": "^3.7.0",
    "p5": "^1.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router": "^6.2.1",
    "react-router-dom": "^6.2.1",
    "swr": "^1.1.1"
  },
  "devDependencies": {
    "@types/p5": "^1.3.3",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "apollo": "^2.33.9",
    "esbuild": "^0.14.2",
    "esbuild-plugin-wasm": "^1.0.0",
    "graphql-tag": "^2.12.6",
    "typescript": "^4.5.2",
    "yargs-parser": "^21.0.0"
  }
}
